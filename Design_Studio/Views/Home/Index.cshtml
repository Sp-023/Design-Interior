@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Design_StudiO</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!--Jquery-->
    
    <style>
        body{
            background-color:black;
        }
        .back {
            background-image: url("https://th.bing.com/th/id/R.02dd34983d185d8a28218be838caed07?rik=r6j43It4eXPUYw&riu=http%3a%2f%2fcdn.home-designing.com%2fwp-content%2fuploads%2f2014%2f06%2fmoder-villa-night-lighting.jpg&ehk=JI4WCp3lSgkLOoy4UnxW5nUFCrIQh%2b0POwkQ%2fjlIooY%3d&risl=&pid=ImgRaw&r=0");
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-size: cover;
            align-items: center;
        }
        h2 {
            font-size: 70px;
            padding: 5%;
            font-family: 'Times New Roman', Times, serif;
            text-align: center;
            color: whitesmoke;
        }
        @*button{
           font-size:40px;
           text-align:center;
           padding:6%;
           color:rgb(241 228 31);
        }*@
        .box1 {
            border: hidden;
            background: rgb(2,0,36);
            background: linear-gradient(90deg, rgba(2,0,36,1) 0%, rgba(162,170,172,1) 52%);
            border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%;
            width: 60%;
            height: 60vh;
        }
        
       
        
    </style>
</head>
<body>
    <div class="back rounded">
        <h2>Welcome to Design Studio</h2>
        <div class="text-center" style="padding:4%;">
            <button type="button" class="btn btn-dark text-white"><a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Design">Get's Started</a></button>
        </div>

    </div>
    <hr />
    <div class="rect">
        <div class="pic">

        </div>
        <div class="text-body">

        </div>
    </div>
    <hr /><br />

    @*This is kitchen *@
    <div class="card mb-3" style="max-width:100%;">
        <div class="row g-0">
            <div class="col-md-4">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSh1Q2UhlpzrxcdsJFH9RFe7SGmVp_0JdAimw&usqp=CAU" class="img-fluid rounded-start" alt="" style="Width:100%;height:50vh;">
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title">Card title</h5>
                    <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>
        </div>
    </div>

    @*This is bedroom*@
    <div class="card mb-3" style="max-width:100%;">
        <div class="row g-0">
            <div class="col-md-4">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSY4pfs_R5abFbvb-ll9SH38g2-U6bNUtGM1Q&usqp=CAU" class="img-fluid rounded-start" alt="" style="Width:100%;height:50vh;">
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title">Card title</h5>
                    <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>
        </div>
    </div>
    @*This is Hall*@
    <div class="card mb-3" style="max-width:100%;">
        <div class="row g-0">
            <div class="col-md-4">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTe9wkEjJpUOUms5i6TlzQ9koKgTqPoZ79oAg&usqp=CAU" class="img-fluid rounded-circle" alt="" style="Width:100%;height:50vh;">
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title">Card title</h5>
                    <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>
        </div>
    </div>


    <div class="box1">
        <h3>hellow this ..</h3>
    </div>
</body>
</html>